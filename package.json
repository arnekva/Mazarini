        "node-geocoder": "^4.2.0",
        "node-schedule": "^2.0.0",
        "nodemon": "^2.0.19",
        "npm": "latest",
        "pm2-axon": "^4.0.1",
        "pm2": "5.0.3",
        "puppeteer": "^24.0.0",
        "request": "^2.88.2",
        "rimraf": "^3.0.2",
        "sharp": "^0.33.5",
        "spotify-web-api-node": "^5.0.0",
        "striptags": "^3.2.0",
        "typescript": "^4.3.2",
        "ultimate-text-to-image": "^1.0.1",
        "wikijs": "^6.4.0"
    },
