{
    "name": "mazarini",
    "version": "18.16.0",
    "description": "",
    "scripts": {
        "start": "nodemon index.ts",
        "add-to-start": "--exec \"npm run lint && node index\"",
        "start:bg": "pm2 --name mazarini start index.ts --watch",
        "start:crashed": "pm2 delete mazarini  && pm2 --name mazarini start index.ts -a crashed",
        "stop:bg": "pm2 delete mazarini",
        "lint": "eslint **/*.ts",
        "test": "mocha -r ts-node/register 'tests/**/*.ts'",
        "create:env": "echo export type env = 'dev' ^| 'prod';export const environment: env = 'dev';export const discordSecret = 'discSecret';export const spotifyClientID  = 'spotifyId';export const spotifyClientSecret = 'spotifyCS';export const mwPw  = 'mwPw';export const lfKey = 'lfKey';export const imgflip = {u: 'user', p: 'pw'};export const actSSOCookie  = 'actSSOCookie ';export const weatherAPIKey = 'weatherAPIKey' > client-env.ts ",
        "build": "npm run create:env && npm test",
        "restart": "npm run stop:bg && git pull && npm run start:bg || git reset --hard $2 && npm run start:bg"
    },
    "keywords": [],
    "author": "Arne Kvaleberg",
    "license": "ISC",
